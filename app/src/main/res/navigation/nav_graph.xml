<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="org.ww.ai.activity.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_RenderResultsFragment"
            app:destination="@id/RenderResultsFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="org.ww.ai.activity.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/RenderResultsFragment"
        android:name="org.ww.ai.activity.RenderResultsFragment"
        android:label="@string/render_results_title"
        tools:layout="@layout/render_results_fragment">
        <action
            android:id="@+id/action_RenderResultsFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
        <action
            android:id="@+id/action_RenderResultsFragment_to_ShowRenderDetailsFragment"
            app:destination="@id/ShowRenderDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/ShowRenderDetailsFragment"
        android:name="org.ww.ai.activity.RenderDetailsFragment"
        android:label="@string/render_details_title"
        tools:layout="@layout/render_details_fragment">
        <action
            android:id="@+id/action_ShowRenderDetailsFragment_to_RenderResultsFragment"
            app:destination="@id/RenderResultsFragment" />
    </fragment>

</navigation>